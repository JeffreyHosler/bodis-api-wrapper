{
    "name": "jeffreyhosler/bodis-api-wrapper",
    "description": "A Lightweight Laravel Wrapper for the Bodis API",
    "keywords": [
        "JeffreyHosler",
        "laravel",
        "bodis-api-wrapper",
		"bodis",
		"bodis api",
		"laravel bodis",
		"laravel bodis api"
    ],
    "homepage": "https://github.com/jeffreyhosler/bodis-api-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "JeffreyHosler",
            "email": "hoslerj2@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
		"guzzlehttp/guzzle": "^6.5|^7.0",
		"illuminate/support": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "JeffreyHosler\\BodisApiWrapper\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeffreyHosler\\BodisApiWrapper\\BodisApiWrapperServiceProvider"
            ],
            "aliases": {
                "Bodis": "JeffreyHosler\\BodisApiWrapper\\Facades\\Bodis"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}